{
    "name": "carlosv2/dumbsmart-repositories",
    "type": "library",
    "description": "Simplistic layer to store documents into their own repositories",
    "keywords": ["Repository", "Filesystem", "Test"],
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Ortega",
            "email": "carlosV2.0@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "carlosV2\\DumbsmartRepositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\carlosV2\\DumbsmartRepositories\\": "spec/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:HummingbirdTechGroup/Communism.git"
        },        
        {
            "type": "vcs",
            "url":  "git@github.com:HummingbirdTechGroup/persisted-objects.git"
        }
    ],
    "require": {
        "php": ">5.3",
        "everzet/persisted-objects": "dev-hb-fork",
        "carlosv2/communism": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "bossa/phpspec2-expect": "~1.0"
    }
}
